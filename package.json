{
  "name": "exceptionbag",
  "description": "Error composition and utility library",
  "version": "1.0.3",
  "author": {
    "name": "Marko Kovacevic",
    "email": "doppelganger113@gmail.com"
  },
  "keywords": [
    "error",
    "exception",
    "composition",
    "utility",
    "axios"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/doppelganger113/exceptionbag"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "files": [
    "./dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "lint-staged": {
    "src/*": "prettier --write --ignore-unknown"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint ./src --ext .ts --quiet --fix",
    "format": "prettier --write ./src/**/*",
    "format:check": "prettier --check ./src/**/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc --project tsconfig.build.json",
    "check": "npm run format && npm run lint && npm test && npm run build"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "reflect-metadata": "0.1.14",
    "axios": "1.6.2",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "express": "4.18.2",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "lint-staged": "15.2.0",
    "prettier": "3.1.0",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "nestjs-otel": "5.1.5"
  }
}
